@echo off

setlocal enabledelayedexpansion
set PC_PATH=%INSTALLED_PATH%\*.pc
set LIBAV_PATH=%INSTALLED_PATH%\libav

@echo %PC_PATH%
@echo %LIBAV_PATH%

FOR /F %%i in ('dir /s /b %PC_PATH%') DO (
@echo %%i
@echo prefix=%LIBAV_PATH% >> %%i.bak
findstr /V "^prefix" %%i >> %%i.bak
move /Y %%i.bak %%i
)
